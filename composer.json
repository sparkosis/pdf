{
    "name": "sparkosis/pdf",
    "description": "Un package gotenberg pour laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Ramos",
            "email": "contact@nicolas-r.fr",
            "homepage": "https://nicolas-r.fr"
        }
    ],
    "homepage": "https://github.com/sparkosis/pdf",
    "keywords": ["Sparkosis", "Pdf"],
    "require": {
        "illuminate/support": "~5",
        "thecodingmachine/gotenberg-php-client": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sparkosis\\Pdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sparkosis\\Pdf\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sparkosis\\Pdf\\PdfServiceProvider"
            ],
            "aliases": {
                "Pdf": "Sparkosis\\Pdf\\Facades\\Pdf"
            }
        }
    }
}
